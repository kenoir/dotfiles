fpath=(/opt/boxen/homebrew/share/zsh/site-functions $fpath)
dotfiles="/Users/sth/Developer/dotfiles"

if [[ -d $dotfiles/zsh/conf.d ]]; then
  for conf in $dotfiles/zsh/conf.d/*.zsh; do
    source $conf
  done
fi

export PATH="/opt/boxen/homebrew/share/npm/bin:/opt/chgems/bin:/opt/vagrant/bin:$PATH"

source /opt/boxen/env.sh
